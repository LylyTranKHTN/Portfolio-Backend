{
  "name": "node-js-get-start",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/src/server.js",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "build": "tsoa spec-and-routes && tsc",
    "start": "node dist/src/server.js",
    "dev": "concurrently \"tsx watch src/server.ts\" \"tsx watch tsoa spec-and-routes\"",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "prepare": "husky",
    "lint": "eslint src"
  },
  "dependencies": {
    "@sequelize/core": "^7.0.0-alpha.41",
    "@sequelize/postgres": "^7.0.0-alpha.41",
    "body-parser": "^1.20.2",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "pg": "^8.12.0",
    "pug": "^3.0.3",
    "sequelize": "^6.37.3",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "@types/swagger-ui-express": "^4.1.6",
    "babel-register": "^6.26.0",
    "concurrently": "^9.0.1",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "3.3.3",
    "sequelize-cli": "^6.6.2",
    "tsx": "^4.19.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.7.0"
  }
}
